    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch server command (non-interactive)",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/packages/server/lib/index.js",
        "args": [],
        "outFiles": [
          "${workspaceFolder}/packages/server/lib/**/*.js",
          "${workspaceFolder}/packages/fx-core/build/**/*.js",
          "${workspaceFolder}/packages/api/build/**/*.js"
        ],
        "resolveSourceMapLocations": [
          "${workspaceFolder}/packages/server/lib/**/*.js",
          "${workspaceFolder}/packages/fx-core/build/**/*.js",
          "${workspaceFolder}/packages/api/build/**/*.js"
        ],
        "console": "integratedTerminal"
      }
    ]
  }