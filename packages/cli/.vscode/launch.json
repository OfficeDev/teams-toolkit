    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch new command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "new",
                "--app-name", "spfxApp",
                "--folder", "./test-folder",
                "--host-type", "SPFx"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ],
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch deploy command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "deploy",
                "--folder", "./test-folder/azureApp0326",
                "--deploy-plugin", "Tab Front-end"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ],
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch login command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "account",
                "login",
                "app-studio"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ],
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch resource add azure-function command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "resource",
                "add",
                "azure-function",
                "--folder", "./test-folder/azureApp",
                "--function-name", "myFunc"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch resource add azure-sql command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "resource",
                "add",
                "azure-sql",
                "--folder", "./test-folder/azureApp",
                "--sql-username", "testuser01",
                "--sql-password", "Test0001"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ]
        },        
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch resource configure aad command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "resource",
                "configure",
                "aad",
                "--aad-env","both",
                "--folder", "./test-folder/azureApp",
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch resource show function command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "resource",
                "show",
                "azure-function",
                "--folder", "./test-folder/azureApp",
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch resource list command",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/cli.js",
            "args": [
                "resource",
                "list",
                "--folder", "./test-folder/azureApp",
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/lib/**/*.js"
            ]
        }
    ]
}