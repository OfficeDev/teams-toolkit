    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.1.0",
