{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "dist/types/src/index.d.ts",
  "docModel": {
    "enabled": true
  },
  "apiReport": {
    "enabled": true,
    "reportFolder": "./review"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "dist/types/<unscopedPackageName>.d.ts",
    "publicTrimmedFilePath": "dist/types/<unscopedPackageName>-public.d.ts"
  },
  "messages": {
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      }
    }
  },
  "newlineKind": "lf"
}
