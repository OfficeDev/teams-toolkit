    "botbuilder-dialogs": "^4.23.1",
    "botframework-connector": "^4.23.1",
    "botframework-schema": "^4.23.1",
    "jwt-decode": "^4.0.0",
    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "@microsoft/teams-js": "^2.19.0",
    "botbuilder": "^4.23.1"
  },
  "devDependencies": {
    "@azure/core-auth": "^1.8.0",
    "@azure/msal-common": "^14.15.0",
