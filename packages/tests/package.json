{
  "name": "@microsoft/teamsfx-test",
  "version": "0.0.1",
  "description": "A UI Test Project of Teams Toolkit Extension",
  "private": true,
  "author": "Microsoft Corporation",
  "license": "MIT",
  "scripts": {
    "build": "rimraf out && tsc -p ./",
    "test:e2e": "npm run test:e2e:clean && npm run test:e2e:parallel",
    "test:e2e:clean": "NODE_OPTIONS=\"--unhandled-rejections=strict\" ts-node src/e2e/clean.ts",
    "test:e2e:parallel": "mocha --parallel --reporter @mochajs/json-file-reporter \"src/e2e/**/*.tests.ts\"",
    "test:e2e:smoke": "mocha --reporter @mochajs/json-file-reporter \"src/e2e/smoke/*.tests.ts\"",
    "test:e2e:others": "mocha --reporter @mochajs/json-file-reporter \"src/e2e/{,!(smoke)}/*.tests.ts\"",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@microsoft/dev-tunnels-connections": "~1.0.7360",
    "@microsoft/dev-tunnels-contracts": "~1.0.7360",
    "@microsoft/dev-tunnels-management": "~1.0.7360",
    "@microsoft/dev-tunnels-ssh": "~3.11.1",
    "@microsoft/extra-shot-mocha": "0.0.1",
    "@microsoft/teamsfx-core": "^2.0.2",
    "@types/chai": "^4.2.19",
    "@types/fs-extra": "^7.0.0",
    "@types/glob": "^7.1.3",
    "@types/keytar": "^4.4.2",
    "@types/md5": "^2.3.0",
    "@types/mocha": "^8.2.2",
    "@types/mochawesome": "^6.2.1",
    "@types/mssql": "^8.1.2",
    "@types/node": "^14.17.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.29.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.1.6",
    "husky": "^8.0.3",
    "lint-staged": "^10.5.4",
    "mocha": "10.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "7.0.1",
    "mock-fs": "^5.1.2",
    "mocked-env": "^1.3.5",
    "mssql": "^9.1.1",
    "mustache": "^4.2.0",
    "nyc": "^15.1.0",
    "playwright": "^1.22.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.2",
    "uuid": "^8.3.2",
    "vscode-extension-tester": "~5.6.0"
  },
  "dependencies": {
    "@azure/arm-apimanagement": "^8.0.0",
    "@azure/arm-resources": "5.0.1",
    "@azure/identity": "^3.1.3",
    "@azure/msal-node": "1.17.3",
    "@azure/arm-sql": "^9.0.0",
    "@microsoft/teamsfx-api": "^0.22.3",
