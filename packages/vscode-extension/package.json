          "when": "fx-extension.sidebarWelcome.treeview"
        },
        {
          "id": "teamsfx-empty-project",
          "name": "Teams Toolkit",
          "when": "fx-extension.sidebarWelcome.default && !fx-extension.canUpgradeToArmAndMultiEnv"
        },
        {
          "id": "teamsfx-empty-project-and-check-upgrade",
          "name": "Teams Toolkit",
          "when": "fx-extension.sidebarWelcome.default && fx-extension.canUpgradeToArmAndMultiEnv"
        }
      ]
